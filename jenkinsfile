pipeline {
        agent any
           stages {
            stage ('one') {
                      steps {
                         echo "building the application"
                      }
                 }
                 satge ('two') {
                           steps {
                               input ('do you want to proceed')
                        }
                  }
                  stage ('three') {
                             when {
                                 not {
                                       'branch master'
                                       }
                                    }
                                    steps {
                                         echo "hello"
}
}


